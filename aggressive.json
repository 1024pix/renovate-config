{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>1024pix/renovate-config:default"],
  "schedule": ["at any time"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "labels": ["renovate", "no-review-app"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "automerge": true
    }
  ]
}
