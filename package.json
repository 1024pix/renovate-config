{
  "name": "renovate-config",
  "version": "0.0.0",
  "private": false,
  "license": "AGPL-3.0",
  "engines": {
    "node": "18",
    "npm": "<10"
  },
  "description": "1024pix shared renovate config",
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "test": "run-p validate:**",
    "validate:default": "RENOVATE_CONFIG_FILE=default.json renovate-config-validator",
    "validate:aggressive": "RENOVATE_CONFIG_FILE=aggressive.json renovate-config-validator",
    "validate:preset:group-by-directory": "RENOVATE_CONFIG_FILE=presets/group-by-directory.json renovate-config-validator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/renovate-config.git"
  },
  "keywords": [
    "renovate",
    "config"
  ],
  "author": "1024pix team",
  "bugs": {
    "url": "https://github.com/1024pix/renovate-config/issues"
  },
  "homepage": "https://github.com/1024pix/renovate-config#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "renovate": "^35.0.0"
  }
}
