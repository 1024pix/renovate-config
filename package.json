{
  "name": "renovate-config",
  "version": "0.0.0",
  "private": false,
  "license": "AGPL-3.0",
  "engines": {
    "node": "^v18.16.1",
    "npm": "<10"
  },
  "description": "1024pix shared renovate config",
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "test": "run-p validate:**",
    "validate:default": "renovate-config-validator default.json",
    "validate:aggressive": "renovate-config-validator aggressive.json",
    "validate:auto-patch": "renovate-config-validator auto-patch.json",
    "validate:auto-minor": "renovate-config-validator auto-minor.json",
    "validate:presets": "renovate-config-validator presets/*.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1024pix/renovate-config.git"
  },
  "keywords": [
    "renovate",
    "config"
  ],
  "author": "1024pix team",
  "bugs": {
    "url": "https://github.com/1024pix/renovate-config/issues"
  },
  "homepage": "https://github.com/1024pix/renovate-config#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "renovate": "^35.0.0"
  }
}
