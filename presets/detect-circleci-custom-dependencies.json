{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["^.circleci/config.yml$"],
      "matchStrings": [
        "# renovate datasource=(?<datasource>.*?) depName=(?<depName>.*?)\\s*[a-zA-Z]+: (?<currentValue>.*)\\s"
      ]
    }
  ]
}
